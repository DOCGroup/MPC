conditional_include "bmakecommon"

startup_letter   = x
use_exe_modifier =

Release {
  optimize         = 1
  defines          = NDEBUG
  exeoutputdir     = Release
  intermediate_dir = Release
  postlinkrmext    = .tds
}

Debug {
  optimize         =
  debug_prj        = 1
  defines          = _DEBUG
  exeoutputdir     = .
  intermediate_dir = Debug
  lib_modifier     = d
  postlinkrmext    = .tds
}

conditional_include "user_bmakedllexe"
