// -*- MPC -*-
// $Id$

project {
  requires += qt
  includes += $(QTDIR)/include
  libpaths += $(QTDIR)/lib
  macros   += QT_THREAD_SUPPORT

  // @@ Notice: If you are building with Cygwin, you may need to manually
  //            change the following Qt library to qt-mt230nc.
  specific(gnuace, make, sle, automake, ghs) {
    lit_libs += qt-mt
  } else {
    lit_libs += qt-mt230nc
  }
}
