// $Id$
project: wxbase, winapp {
  specific(prop:windows) {                                  
    Debug::lit_libs   += wx$(WX_PORT)$(WX_VERSION)$(WX_UNICODE)d_core
    Release::lit_libs += wx$(WX_PORT)$(WX_VERSION)$(WX_UNICODE)_core
  } else {
    lit_libs += wx_$(WX_PORT)$(WX_UNICODE)_core$(WX_VERSION)
  }
}
