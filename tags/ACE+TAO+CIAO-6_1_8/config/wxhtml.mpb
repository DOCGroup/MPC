// $Id$
project: wxcore {
  specific(prop:windows) {                                  
    Debug::lit_libs   += wx$(WX_PORT)$(WX_VERSION)$(WX_UNICODE)d_html
    Release::lit_libs += wx$(WX_PORT)$(WX_VERSION)$(WX_UNICODE)_html
  } else {
    lit_libs += wx_$(WX_PORT)$(WX_UNICODE)_html$(WX_VERSION)
  }
}
